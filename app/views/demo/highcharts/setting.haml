!!! Strict
%html
  %head
    %meta{:content => "text/html; charset=utf-8", "http-equiv" => "Content-Type"}/
    %link{href: "#{asset_link_with_cdn(Settings.website.favicon)}", rel: "shortcut icon"}
    %title Highcharts Examples
    = javascript_include_tag_with_cdn "jquery.min.js"
    = javascript_include_tag_with_cdn "bootstrap.min.js"
    = stylesheet_link_tag_with_cdn "bootstrap.min.css"
    :css
      a:hover { text-decoration: none; }
      a:link { text-decoration: none; }
      .string { color: green; }
      .number { color: darkorange; }
      .boolean { color: blue; }
      .null { color: magenta; }
      .key { color: red; }
      table tr td { text-align: center; }
  %body
    %nav.navbar.navbar-default#pageSetting
      .container
        .navbar-header
          %a.navbar-brand SOLife
        .collapse.navbar-collapse
          %ul.nav.navbar-nav.navbar-right
            %li
              %a#refresh Refresh
           
    .container
      .row
        .col-sm-10.col-sm-offset-1
          .alert.alert-info Heads up! Back to this view when tap three touched anywhere.
      .row
        .col-sm-10.col-sm-offset-1
          #accordion.panel-group{"aria-multiselectable" => "true", :role => "tablist"}
            .panel.panel-default
              #headingOne.panel-heading{:role => "tab"}
                %h4.panel-title
                  %a{"aria-controls" => "collapseOne", "aria-expanded" => "true", "data-parent" => "#accordion", "data-toggle" => "collapse", :href => "#collapseOne", :role => "button"}
                    Demo List 
              #collapseOne.panel-collapse.collapse.in{"aria-labelledby" => "headingOne", :role => "tabpanel"}
                .panel-body
                  %table.table.table-condensed.table-strip
                    %thead
                      %tr
                        %td{width:"55%"} filename
                        %td{width:"15%"} size
                        %td{width:"15%"} action
                        %td{width:"15%"} view
                    %tbody#demoList

          #accordion.panel-group{"aria-multiselectable" => "true", :role => "tablist"}
            .panel.panel-default
              #headingTwo.panel-heading{:role => "tab"}
                %h4.panel-title
                  %a{"aria-controls" => "collapseTwo", "aria-expanded" => "true", "data-parent" => "#accordion", "data-toggle" => "collapse", :href => "#collapseTwo", :role => "button"}
                    Setting 
              #collapseTwo.panel-collapse.collapse.in{"aria-labelledby" => "headingTwo", :role => "tabpanel"}
                .panel-body
                  %table.table.table-condensed.table-strip
                    %tbody
                      %tr
                        %td 
                          %a{onclick: "IOSBridge.refreshPage();"} Refresh this page
                      %tr
                        %td 
                          Demo's home page should `index.html`.
                      %tr
                        %td
                          Demo's inner link `not ignore case`.


            .panel.panel-default
              #headingLog.panel-heading{:role => "tab"}
                %h4.panel-title
                  %a{"aria-controls" => "collapseLog", "aria-expanded" => "true", "data-parent" => "#accordion", "data-toggle" => "collapse", :href => "#collapseLog", :role => "button"}
                    IOS Log 
                  %a{style:"float:right;", onclick: "IOSBridge.cleanLog();"} clean
              #collapseLog.panel-collapse.collapse.in{"aria-labelledby" => "headingLog", :role => "tabpanel"}
                .panel-body#log
                 


    = javascript_include_tag_with_cdn "demo_highcharts_ios.js"